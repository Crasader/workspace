package com.zyw.controller;

public class Main {
	
	public static void main(String[] args) {
		
		Controller.login();
		try {
			Thread.sleep(5);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		Controller.queryShop(Controller.queryShopList(null));
	}

}
